{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "amd": true
  },
  "rules": {
    "react/jsx-key": 0,
    "react/display-name": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": false,
    "react/no-unknown-property": false,
    "no-unused-vars": 1,
    "no-console": [
      "error", {
        "allow": [
          "log",
          "warn",
          "error"
        ]
      }
    ]
  }
}
